<div class="container">
<!--content -->
<h3>Bobot Preferensi</h3>
<div class="row">
    <form action="/saw" method="POST" class="col s12">
        <div class="row">
            <div class="input-field">
                <input type="text" id="kode" name="kode">
                <label for="kode">Kode</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="nnr" name="nnr">
                <label for="nnr">Min Nilai Rata-rata</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="jpo" name="jpo">
                <label for="jpo">Jumlah Penghasilan Orang Tua</label>
            </div>
        </div>
         <div class="row">
            <div class="input-field">
                <input type="text" id="jto" name="jto">
                <label for="jto">Jumlah Tanggungan Orang Tua</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="bobot" name="bobot">
                <label for="bobot">Bobot</label>
            </div>
        </div>
        <div class="row">
            <input type="submit" value="Save" class="btn">
            <a href="/dashboardpreferensi" class="btn orange">Cancel</a>
        </div>
    </form>
</div>

<!-- others source -->
